{
  "name": "gpt-discord-bot",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "private": true,
  "scripts": {
    "register": "tsx ./src/register.ts",
    "dev": "tsx watch ./src/server.ts",
    "format": "biome format --write ./src",
    "build": "tsup-node --clean ./src/server.ts",
    "start": "node ./dist/server.cjs"
  },
  "dependencies": {
    "@discordjs/rest": "^2.3.0",
    "discord-interactions": "^4.0.0",
    "discord.js": "^14.15.3",
    "openai": "^4.52.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@types/node": "^20.14.10",
    "dotenv": "^16.4.5",
    "tsup": "^8.1.0",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3"
  }
}
{
  "name": "gpt-discord-bot",
  "version": "0.0.1",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "deploy-commands": "tsx ./src/deploy-commands.ts",
    "dev": "tsx watch ./src/main.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "tsup-node --format=esm ./src/main.ts",
    "start": "node ./dist/main.js"
  },
  "dependencies": {
    "@discordjs/rest": "^1.5.0",
    "discord.js": "^14.7.1",
    "openai": "^3.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.14.2",
    "dotenv": "^16.0.3",
    "prettier": "^2.8.4",
    "ts-node-dev": "^2.0.0",
    "tsup": "^6.6.3",
    "tsx": "^3.12.3",
    "typescript": "^4.9.5"
  }
}

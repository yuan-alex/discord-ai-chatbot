{
  "name": "gpt-discord-bot",
  "version": "0.0.1",
  "type": "module",
  "license": "MIT",
  "private": true,
  "scripts": {
    "register": "tsx ./src/register.ts",
    "dev": "tsx watch ./src/server.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "tsup-node --clean ./src/server.ts",
    "start": "node ./dist/server.cjs"
  },
  "dependencies": {
    "@discordjs/rest": "^2.2.0",
    "@t3-oss/env-core": "^0.7.1",
    "discord-interactions": "^3.4.0",
    "discord.js": "^14.14.1",
    "openai": "^4.20.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "dotenv": "^16.3.1",
    "prettier": "^3.1.0",
    "tsup": "^8.0.1",
    "tsx": "^4.5.0",
    "typescript": "^5.3.2"
  }
}
